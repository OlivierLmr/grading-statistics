[run]
source = .
omit =
    tests/*
    .venv/*
    venv/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == \.__main__\.:
    if TYPE_CHECKING:
    @abstractmethod

# Coverage target: 90% (currently building test suite)
# fail_under = 90

[html]
directory = htmlcov
